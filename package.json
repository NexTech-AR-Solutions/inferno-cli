  "bin": {
    "inferno": "./bin/run"
  },
  "bugs": "https://github.com/novologic/inferno-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "cli-ux": "^5.5.0",
    "cosmiconfig": "^7.0.0",
    "fs-extra": "^9.0.1",
    "js-beautify": "^1.13.0",
    "moment": "^2.29.0",
    "node-notifier": "^8.0.0",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.35",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
